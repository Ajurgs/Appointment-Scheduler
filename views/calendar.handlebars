<script
  src="https://cdn.jsdelivr.net/npm/fullcalendar@5.7.0/main.min.js"
></script>
<script>

  document.addEventListener('DOMContentLoaded', function() { var calendarEl =
  document.getElementById('calendar'); var calendar = new
  FullCalendar.Calendar(calendarEl, { timeZone: 'UTC', initialView:
  'resourceTimeGridDay', resources: [{ id: 'a' , title: 'location a' }, { id:
  'b', title: 'Room B'}, { id: 'c', title: 'Room C' }, { id: 'd', title: 'Room D' } ], events:
  'https://fullcalendar.io/demo-events.json?with-resources=4&single-day'});
  calendar.render(); });

</script>
<div id="calendar"></div>
<script src="/js/main.js"></script>